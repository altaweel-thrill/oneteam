  <script src="https://unpkg.com/gsap@3/dist/gsap.min.js"></script>

  <script>
    const teamMembers = [
      { name: "Mr. Ali", role: "Baba", color: "purple", image: "/images/ali.png", height: "200px" },
      { name: "Nadia", role: "Lestary", color: "green", image: "/images/nadia.png", height: "300px" },
      { name: "Evan", role: "Caster Lee", color: "purple", image: "/images/33.png", height: "250px" },
      { name: "Stefy", role: "Catlyna", color: "green", image: "/images/44.png", height: "250px" },
       { name: "Mr. Ali", role: "Baba", color: "purple", image: "/images/ali.png", height: "200px" },
      { name: "Nadia", role: "Lestary", color: "green", image: "/images/nadia.png", height: "300px" },

    ];

    const container = document.getElementById("team");

    teamMembers.forEach(member => {
      const card = document.createElement("div");
      card.className = `team-card ${member.color}`;

      card.innerHTML = `
        <div>
          <h3>${member.name}</h3>
          <span>${member.role}</span>
        </div>

        <div class="shape"></div>

        <img style="width:100%; height:${member.height}%; object-fit:cover;" src="${member.image}" height="${member.height}" alt="${member.name}">
      `;

      container.appendChild(card);
    });

    const cards = document.querySelectorAll(".team-card");

    /* First card active */
    cards[0].classList.add("active");

    /* Hover glow switch */
    cards.forEach(card => {
      card.addEventListener("mouseenter", () => {
        cards.forEach(c => c.classList.remove("active"));
        card.classList.add("active");
      });

      card.addEventListener("mouseleave", () => {
        cards.forEach(c => c.classList.remove("active"));
        cards[0].classList.add("active");
      });
    });

    /* GSAP entrance */
    gsap.from(".team-card", {
      y: 40,
      opacity: 0,
      duration: 0.8,
      ease: "power3.out",
      stagger: 0.12
    });

    /* Mouse wheel â†’ horizontal scroll */
    const scrollContainer = document.querySelector(".scroll-container");

    scrollContainer.addEventListener("wheel", (e) => {
      e.preventDefault();
      scrollContainer.scrollLeft += e.deltaY * 1.2;
    }, { passive: false });
  </script>

                            
                           
                            
                            
                            
                            
                        
